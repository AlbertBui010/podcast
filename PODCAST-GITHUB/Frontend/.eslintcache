;\n\nimport PhatTrienSuNghiep from \"./components/content/PhatTrienSuNghiep\"\nimport PhatTrienSuNghiepDetails from \"./components/content/PhatTrienSuNghiepDetails\"\n\nconst App = () => {\n  useEffect(() => {\n    store.dispatch(loadUser());    \n  }, []);\n  \n\n  return (\n    <Router>\n      <div className=\"App\">\n        {/*<Header />*/}\n        <Header />\n        <div>\n          <Route path=\"/\" component={Home} exact />\n          <Route path=\"/search/:keyword\" component={Home} />\n\n         \n          <Route path=\"/phat-trien-su-nghiep\" component={PhatTrienSuNghiep} exact/>\n          <Route path=\"/phat-trien-su-nghiep/:id\" component={PhatTrienSuNghiepDetails} exact/>\n          <Route path=\"/login\" component={Login} />\n          \n          <Route path=\"/register\" component={Register} />\n          <ProtectedRoute path=\"/me\" component={Profile} exact />\n          <ProtectedRoute path=\"/me/update\" component={UpdateProfile} exact />\n          <ProtectedRoute\n            path=\"/password/update\"\n            component={UpdatePassword}\n            exact\n          />\n\n        </div>\n\n        \n        <ProtectedRoute\n          path=\"/admin/contents\"\n          isAdmin={true}\n          component={ContentsList}\n          exact\n        />\n        <ProtectedRoute\n          path=\"/admin/content\"\n          isAdmin={true}\n          component={NewContent}\n          exact\n        />\n        <ProtectedRoute\n          path=\"/admin/content/:id\"\n          isAdmin={true}\n          component={UpdateContent}\n          exact\n        />\n      \n        \n        <ProtectedRoute\n          path=\"/admin/users\"\n          isAdmin={true}\n          component={UsersList}\n          exact\n        />\n\n        <ProtectedRoute\n          path=\"/admin/user/:id\"\n          isAdmin={true}\n          component={UpdateUser}\n          exact\n        />\n       \n\n        {/* {!loading && (!isAuthenticated || user.typeRole !== 'adm